<Window x:Class="ArgesDataCollectionWithWpf.UI.UIWindows.CustomerUserControl.SuJiTableSettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArgesDataCollectionWithWpf.UI.UIWindows.CustomerUserControl"
        mc:Ignorable="d"
        Title="塑机类型拉杆类型管理表设置" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="131*"></RowDefinition>
            <RowDefinition Height="14*"></RowDefinition>
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="0" x:Name="dataGrid_SuJiSetting"   AutoGenerateColumns="True" CanUserAddRows="true" CanUserDeleteRows="true" />

        <Button Grid.Row="1" Name="btn_SaveExcel" Content="保存" Click="btn_SaveExcel_Click" Width="100"> </Button>

    </Grid>
</Window>
